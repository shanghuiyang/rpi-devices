{
    "zoomInButtonPin": 18,
    "zoomOutButtonPin": 23,
    "gps": {
        "dev": "/dev/ttyAMA0",
        "baud": 9600,
        "defaultLoc": {
            "lat": 39.956767,
            "lon": 116.447697
        },
        "simulator": {
            "enable": false,
            "source": "gps.csv"
        }
    },
    "display": {
        "res": 22,
        "dc": 17,
        "blk": 27,
        "width": 240,
        "height": 240,
        "sleepAfterMin": 5
    },
    "iot": {
        "enable": false,
        "onenet": {
            "token": "your_onenet_token",
            "api": "http://api.heclouds.com/devices/540381180/datapoints"
        }
    },
    "tile": {
        "minZoom": 0,
        "maxZoom": 18,
        "defaultZoom": 16,
        "tileProviders": [
            "osm-outdoor",
            "bing-satellite"
        ],
        "defaultTileProvider": "osm-outdoor",
        "cacheDir": ".cache/maptiles",
        "online": false
    }
}
